    "description": "PHP library for extracting and replacing string data in .docx files.",
    "keywords": ["Docx", "Extractor", "Mapper"],
    "homepage": "https://github.com/Label305/DocxExtractor",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Thijs Scheepers",
            "email": "thijs@label305.com",
            "role": "Developer"
        }
    ],
    "require": {
